{
  "expo": {
    "name": "ProfitPilot",
    "slug": "profitpilot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bytebeem.App"
    },
    "web": {
      "favicon": "./assets/splash.png"
    },
    "extra": {
      "eas": {
        "projectId": "e5540f47-257d-4d03-ae6e-1cb02d6b5e8c"
      }
    },
    "plugins": [
      "expo-secure-store"
    ],
    "owner": "bytebeem"
  }
}
